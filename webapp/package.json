{
  "name": "webapp",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build",
    "build:server": "esbuild ./server/index.ts --bundle --external:vite --external:../build/server/index.js --outdir=./server-build --platform=node --target=node20 --sourcemap --format=esm --log-level=info --packages=external",
    "dev": "NODE_ENV=development FORCE_COLOR=true tsx watch --clear-screen=false --ignore \".cache/**\" --ignore \"app/**\" --ignore \"vite.config.ts.timestamp-*\" --ignore \"build/**\" --ignore \"node_modules/**\" -r dotenv/config --inspect ./server/index.ts | pino-pretty",
    "start": "node ./server-build/index.js",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc",
    "ci:prepare": "run-s typecheck lint:fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node ./scripts/migrate.mjs",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@rainbow-me/rainbowkit": "^2.1.2",
    "@remix-run/express": "^2.10.0",
    "@remix-run/node": "^2.10.0",
    "@remix-run/react": "^2.10.0",
    "@remix-run/serve": "^2.10.0",
    "@t3-oss/env-core": "^0.10.1",
    "@tanstack/react-query": "^5.49.2",
    "address": "^2.0.3",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "close-with-grace": "^1.3.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "get-port": "^7.1.0",
    "helmet": "^7.1.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.399.0",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "postgres": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "validate-cli": "workspace:*",
    "viem": "^2.13.1",
    "wagmi": "^2.10.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@remix-run/dev": "^2.10.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.22.8",
    "esbuild": "^0.22.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.2.1",
    "postcss": "^8.4.38",
    "remix-flat-routes": "^0.6.5",
    "remix-routes": "^1.7.6",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.7.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
